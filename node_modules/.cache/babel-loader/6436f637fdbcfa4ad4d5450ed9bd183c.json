AA,qBAAA,IAAA,EAAA,KAAA,EAAA;AACA,UAAA,MAAA,GAAA,IAAA,UAAA,EAAA;AACA,UAAA,EAAA,GAAA,IAAA;AACA,UAAA,GAAA,GAAA,EAAA;;AACA,MAAA,MAAA,CAAA,MAAA,GAAA,UAAA,CAAA,EAAA;AACA,QAAA,GAAA,CAAA,QAAA,GAAA,CAAA,CAAA,MAAA,CAAA,MAAA;AACA,QAAA,GAAA,CAAA,UAAA,GAAA,IAAA;AACA,QAAA,GAAA,CAAA,IAAA,GAAA,IAAA,CAAA,IAAA,CAHA,CAKA;;AACA,YAAA,EAAA,CAAA,eAAA,CAAA,GAAA,CAAA,EAAA;AACA,UAAA,EAAA,CAAA,WAAA;AACA,SAFA,MAEA;AACA,UAAA,EAAA,CAAA,aAAA,CAAA,GAAA,EAAA,KAAA;AACA;AACA,OAXA;;AAYA,MAAA,MAAA,CAAA,aAAA,CAAA,IAAA;AACA,KA7CA;AA8CA,IAAA,eAAA,EAAA,yBAAA,KAAA,EAAA;AACA,aACA,KAAA,CAAA,QAAA,KAAA,EAAA,IAAA,MAAA,CAAA,IAAA,CAAA,KAAA,CAAA,UAAA,EAAA,MAAA,KAAA,CAAA,IAAA,KAAA,CAAA,IAAA,KAAA,EADA;AAGA,KAlDA;AAmDA,IAAA,uBAnDA,qCAmDA;AACA,WAAA,OAAA,CAAA,IAAA,CACA,kBADA,EAEA,YAAA,CAAA,CAFA,EAGA,YAAA,CAAA,CAHA;AAKA,KAzDA;AA0DA,IAAA,WAAA,EAAA,uBAAA;AACA,WAAA,QAAA,GAAA,mBAAA;AACA,WAAA,YAAA,GAAA,IAAA;AACA;AA7DA;AAdA,CAAA","sourcesContent":["<template>\n  <div class=\"c-page-container\">\n    <div class=\"c-page-title\">\n      <p>商品１情報</p>\n    </div>\n    <div class=\"c-page-row center\" @click=\"fileClick()\">\n      <div class=\"ribbon\">\n          スマホで簡単査定\n      </div>\n      <button class=\"c-photo-btn\">\n        <span class=\"iconfont icon-camera\"></span>\n      </button>\n      <div class=\"c-photo-add-btn\">\n        + 商品を追加する\n      </div>\n    </div>\n    <span class=\"error-msg\" v-show=\"showErrorMsg\">{{ errorMsg }}</span>\n    <input\n      id=\"upload_file\"\n      multiple=\"multiple\"\n      type=\"file\"\n      accept=\"image/*\"\n      @change=\"onFileChange($event)\"\n    />\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport { mapActions } from 'vuex'\nexport default {\n  name: \"Photo\",\n  data() {\n    return {\n      errorMsg: \"\",\n      showErrorMsg: false\n    };\n  },\n  computed: {\n    ...mapGetters({\n      imageData: 'itemInformation/getImageData',\n      imageList: 'itemInformation/getImageList'\n    })\n  },\n  methods: {\n    ...mapActions({\n      saveStoreImageData: 'itemInformation/saveImageData',\n      saveStoreImageList: 'itemInformation/saveImageList'\n    }),\n    saveImageData(image, index) {\n      /* 各商品の画像オブジェクトに保存 */\n      this.imageData.splice(index, 1, image);\n      this.saveStoreImageData(this.imageData);\n\n      /* 全体の商品イメージリストに保存する */\n      this.imageList.splice(0,1, this.imageData)\n      this.saveStoreImageList(this.imageList)\n\n      /* 商品情報ページに遷移 */\n      this.openItemInformationPage();\n    },\n    fileClick: function() {\n      $(\"#upload_file\").click();\n    },\n    onFileChange: function(e) {\n      var files = e.target.files || e.dataTransfer.files;\n      if (files.length > 0) {\n        files.forEach((file, index) => {\n          this.createImage(file, index);\n        });\n      }\n    },\n    createImage: function(file, index) {\n      var reader = new FileReader();\n      var vm = this;\n      var obj = {};\n      reader.onload = function(e) {\n        obj.thumnail = e.target.result;\n        obj.uploadFile = file;\n        obj.name = file.name;\n\n        // アップロード成功すれば保存する\n        if (vm.checkEmptyImage(obj)) {\n          vm.setErrorMsg();\n        } else {\n          vm.saveImageData(obj, index);\n        }\n      };\n      reader.readAsDataURL(file);\n    },\n    checkEmptyImage: function(image) {\n      return (\n        image.thumnail === \"\" && Object.keys(image.uploadFile).length === 0 && image.name === \"\"\n      );\n    },\n    openItemInformationPage() {\n      this.$router.push(\n        \"item_information\",\n        () => {},\n        () => {}\n      );\n    },\n    setErrorMsg: function() {\n      this.errorMsg = \"クリックして写真を登録してください\";\n      this.showErrorMsg = true;\n    }\n  }\n};\n</script>\n<style>\n.error-msg {\n  color: red;\n  font-size: 16px;\n  margin-top: 10px;\n}\n</style>\n"],"sourceRoot":"src/pages"}]}